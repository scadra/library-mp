{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"],
    "types": [
      "vite/client",
      "vite-plugin-pwa/client",
      "vite-plugin-pages/client",
      "@intlify/vite-plugin-vue-i18n/client"
    ],
    "paths": {
      "/~/*": ["./src/assets/*"],
      "/@src/*": ["./src/*"]
    }
  },
  "paths": {
    "@/*": [
      "src/*"
    ],
    "Models/*": [
      "src/models/*"
    ],
    "Components/*": [
      "src/components/*"
    ],
    "Constants/*": [
      "src/constants/*"
    ],
    "Interfaces/*": [
      "src/services/interfaces/*"
    ],
    "Services/*": [
      "src/services/impl/*"
    ],
    "Utils/*" : [
      "src/utils/*"
    ],
  },
  "include": [
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "documentation/**/*.md"
  ]
}